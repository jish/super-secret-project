
<div id="commits"></div>

<script type="text/javascript" charset="utf-8">
  $(function() {
    $.get('/commits.json', function(data) {
      $(data).each(function(index, commit) {
        var commitElement = $('<div class="commit">' + commit.message + '</div>');
        $('#commits').append(commitElement);
      });
    });
  })
</script>
